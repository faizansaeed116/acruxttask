{
  "apps": [
    {
      "name": "ETIOP",
      "version": "0.0.0",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "script": "./bin/www",
      "instances": 1,
      "instance_var": "INSTANCE_ID",
      "exec_mode": "cluster",
      "watch": true,
      "ignore_watch": [
        "logs",
        "public",
        "node_modules",
        ".git",
        "sessions"
      ],
      "watch_options": {
        "usePolling": false,
        "alwaysStat": false,
        "useFsEvents": true
      },
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000,
        "MONITORING": true,
        "UV_THREADPOOL_SIZE": 50,
        "app_version": "1.0"
      }
    }
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "generic-pool": "^3.7.1",
    "http-errors": "~1.6.3",
    "knex": "^0.20.4",
    "log4js": "^5.3.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "oracledb": "^4.1.0",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.0",
    "passport-local": "^1.0.0",
    "pm2": "^4.4.1",
    "session-file-store": "^1.3.1",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.6"
  }
}
